<?xml version="1.0" encoding="utf-8"?>
<project outputprefix="kblcd">

    <!--  Project description -->
    <name>Demo - Klávesnice a LCD</name>
    <author>Zdeněk Vašíček</author>
    <authoremail>vasicek@fit.vutbr.cz</authoremail>
    <revision>20090201</revision>
    <description>Aplikace demonstrující využití LCD displeje a klávesnice. Naposledny stisknutá klávesa je uvedena na LCD displeji.</description>

    <!--  MCU part -->
    <mcu>
        <include>mcu/libs/keyboard/package.xml</include>
        <include>mcu/libs/lcd/package.xml</include>
        <file>main.c</file>
    </mcu>

    <!-- FPGA part -->
    <fpga>
        <include>fpga/ctrls/keyboard/package.xml</include>
        <include>fpga/ctrls/lcd/package.xml</include>
        <file>top_level.vhd</file>
    </fpga>

</project>
